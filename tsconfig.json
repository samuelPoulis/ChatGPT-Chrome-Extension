{
    "compilerOptions": {
       "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }, 
    "moduleResolution": "node",
      "strict": true,
      "module": "es6",
      "target": "es6",
      "esModuleInterop": true,
      "sourceMap": true,
      "rootDir": "src",
      "outDir": "dist/js",
      "noEmitOnError": true,
      "jsx": "react"
    }
  }